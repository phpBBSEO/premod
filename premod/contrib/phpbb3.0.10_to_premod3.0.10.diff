+	function validate_num($value, $key, $float = 1, $min = 0 , $max = 4) {
+		$float = $float > 0 ? (int) $float : 0;
+		$min = $min > 0 ? $min : 0;
+		$max = $max > 0 ? $max : 4;
+		$value = ($value >= $min && $value <= $max) ? $value : ($max/2);
