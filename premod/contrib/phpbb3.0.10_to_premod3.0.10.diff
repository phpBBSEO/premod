+				$mem_stats = $this->mem_usage();
+				$this->outputs['gen_out'] = sprintf('%.5f', $phpbb_seo->microtime_float() - $this->outputs['microtime']);
+				$genstats = sprintf($this->gym_master->style_config['stats_nocache'], $this->outputs['gen_out'], $mem_stats);
