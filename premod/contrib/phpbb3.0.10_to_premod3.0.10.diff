+		$forum_id = (int) $row['forum_id'];
+		$topic_id = (int) $row['topic_id'];
+		$_parent = $row['topic_type'] == POST_GLOBAL ? $phpbb_seo->seo_static['global_announce'] : $phpbb_seo->seo_url['forum'][$forum_id];
