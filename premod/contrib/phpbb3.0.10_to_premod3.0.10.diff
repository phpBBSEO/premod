+			}
+			trigger_error($message . $this->back_to_prev());
+		}
+		// Clear cache type
+		if ($mode === 'main') {
+			$cache_actions = array( 'gym' => $user->lang['ALL'], 'google' => $user->lang['GOOGLE'], 'rss' => $user->lang['RSS'], /*'yahoo' => $user->lang['YAHOO'], 'html' => $user->lang['HTML']*/);
+		} else {
