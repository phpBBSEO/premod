+$template->assign_block_vars('navlinks', array(
+	'FORUM_NAME' => "Sync Topic URL",
+	'U_VIEW_FORUM'	=> append_sid("./sync_url.$phpEx"))
+);
+$msg_title = $user->lang['SYNC_TITLE'];
