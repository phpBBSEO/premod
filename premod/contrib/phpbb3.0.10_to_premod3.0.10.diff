+				$cat_module_data[$cat_name] = array(
+					'module_id'		=> $this->check_module_id($cat_name, ''),
+					'module_basename'	=> '',
+					'module_enabled'	=> 1,
+					'module_display'	=> 1,
+					'parent_id'		=> 0,
