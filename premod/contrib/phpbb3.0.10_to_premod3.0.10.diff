+		},
+		{
+			string: navigator.platform,
+			subString: "Linux",
+			identity: "Linux"
+		}
+	]
+
+};
+// Browser detect http://www.quirksmode.org/js/detect.html
+onload_functions.push('load_cookie()');
+onunload_functions.push('unload_cookie()');
+
