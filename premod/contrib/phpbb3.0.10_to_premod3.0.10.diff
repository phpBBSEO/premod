+	color: #ffffff;
+	text-decoration: underline;
+}
+#search-box a:active, .navbg a:active, .forumbg .header a:active, .forabg .header a:active, th a:active {
+	color: #ffffff;
+	text-decoration: none;
+}
+/* Links for forum/topic lists */
