+	function clear_all_cache($option = '') {
+		global $phpbb_root_path, $phpEx, $acm_type, $cache;
+		if ($acm_type !== 'file') {
+			// Apparently, we cannot loop through cached variable using cache class in such case, purge all for now
+			$cache->purge();
