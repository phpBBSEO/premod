+a.down		{ background-image: url("{T_IMAGE_PATH}arrow_down.gif") }
+a.left		{ background-image: url("{T_IMAGE_PATH}arrow_left.gif") }
+a.right		{ background-image: url("{T_IMAGE_PATH}arrow_right.gif") }
